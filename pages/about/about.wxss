text,
view {
    box-sizing: border-box
}

.a-head {
    box-sizing: border-box;
    width: 100%;
    padding: 10rpx 30rpx;
    float: left;
    margin-top: 10px;
}

.a-head .left {
    font-size: 24rpx;
}

.a-head .left view {
    line-height: 50rpx;
}

.name {
    font-size: 32rpx;
}

.job {
    color: #999
}

.a-head .right image {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50rpx;
}

.wrap {
    width: 100%;
    padding: 30rpx;
    padding-top: 10rpx;
    float: left;
    box-sizing: border-box;
}

.line {
    width: 100%;
    border-top: 1px solid #e3e3e3;
}

.list-view {
    font-size: 28rpx;
    padding: 20rpx 30rpx 20rpx 55rpx;
    float: left;
    width: 100%;
    box-sizing: border-box;
    box-shadow: inset 0px -1px 1px -1px #e3e3e3;
    color: #555
}

.Motto {
    font-size: 26rpx;
    padding: 10rpx 30rpx;
    float: left
}

.icon-address {
    background: url("data:image/png;base64,") left no-repeat;
    background-size: 50rpx;
}

.icon-mail {
    background: url("data:image/png;base64,") left no-repeat;
    background-size: 40rpx
}

.icon-github {
    background: url("data:image/png;base64,") left no-repeat;
    background-size: 50rpx
}

.icon-blog {
    background: url("data:image/png;base64,") left no-repeat;
    background-size: 50rpx;
}

.audio {
    width: 100%;
    position: absolute;
    left: 0;
    bottom: 0
}

.audio #myAudio {
    width: 750rpx !important;
    background: #fcfcfc;
    bottom: 0
}

.wx-audio-default {
    width: 750rpx !important;
    bottom: 0
}